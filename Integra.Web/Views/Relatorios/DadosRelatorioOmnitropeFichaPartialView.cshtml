    <td data-bind="text: UF_CRM" style="width: 320px; text-align: left;"></td>
    <td data-bind="text: Cuidador" style="width: 320px; text-align: left;"></td>
    <td data-bind="text: Distribuidora" style="width: 320px; text-align: left;"></td>
    <td data-bind="text: Ultima_Compra" style="width: 320px; text-align: left;"></td>

