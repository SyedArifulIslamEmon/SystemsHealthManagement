    <td data-bind="text: Data_Insercao" style="width: 320px; text-align: left;"></td>
    <td data-bind="text: Inserido_Por" style="width: 320px; text-align: left;"></td>
</script>

<script type="text/javascript" language="javascript">
    $(document).ready(function () {
        var view = new relatorioViewModel();
        ko.applyBindingsWithValidation(ko.validatedObservable(view), $("#DivPageDadosRelatorioParcialView").get(0));
		
        $(".ui-dialog-titlebar").hide();
        $(".ui-dialog-buttonpane").hide();        
    });
    
    function relatorioViewModel() {        
        var allData = @Html.Raw(Json.Encode(Model));
        var mappedData = jQuery.map(allData, function (item) { return new ServicosContratadosTableView(item, self); });
        self.DadosRelatorioParcialView = ko.observableArray(mappedData);
		               
        self.generate_excel = function () {
            export_excel("RelatorioDados");
        };  
    }
    
    function ServicosContratadosTableView(data, parent) {
        var self = this;
        self.Parent = parent;

        self.Data_Ficha = ko.observable(data.Data_Ficha);        
        self.Cidade = ko.observable(data.Cidade);   
        self.UF = ko.observable(data.UF);        
        self.Paciente = ko.observable(data.Paciente);
        self.Medico = ko.observable(data.Medico);     
        
        self.CRM = ko.observable(data.CRM);
        self.UF_CRM = ko.observable(data.UF_CRM);        
        self.Cuidador = ko.observable(data.Cuidador);
        self.Distribuidora = ko.observable(data.Distribuidora);        
        self.Ultima_Compra = ko.observable(data.Ultima_Compra);

        self.Telefone1 = ko.observable(data.Telefone1);        
        self.Telefone2 = ko.observable(data.Telefone2);
        self.Data_Contato = ko.observable(data.Data_Contato);        
        self.Status = ko.observable(data.Status);
        self.Tipo_Paciente = ko.observable(data.Tipo_Paciente);     
        
        self.Data_Cadastro = ko.observable(data.Data_Cadastro);
        self.Obs = ko.observable(data.Obs);
        self.Fase_Cadastro = ko.observable(data.Fase_Cadastro);
        self.Adicionado_CRM = ko.observable(data.Adicionado_CRM);
        self.Data_Insercao = ko.observable(data.Data_Insercao);
        self.Inserido_Por = ko.observable(data.Inserido_Por);
    };  
    function export_excel(tableid) {
        var html = '';
        var oTable = $('#'+tableid).dataTable().fnGetData();

        html+="<html><table>";
        html+="<tr style='text-align:left;vertical-align:middle;width:100%'>";
        $('#'+tableid+' thead th').each(function(){html += "<th>" +this.textContent + "</th>"});
        html+="</tr>";
        for (var count = 0; count <= oTable.length - 1; count++) {
            html+="<tr style='text-align:left;vertical-align:middle;width:100%'>";

            html+="<td>"+oTable[count].Data_Ficha.toString()+"</td>" ;
            html+="<td>"+oTable[count].Cidade.toString()+"</td>";
            html+="<td>"+oTable[count].UF.toString()+"</td>" ;
            //html+="<td>"+oTable[count].Paciente.toString()+"</td>";
            html+="<td>"+oTable[count].Medico.toString()+"</td>" ;

            html+="<td>"+oTable[count].CRM.toString()+"</td>";
            html+="<td>"+oTable[count].UF_CRM.toString()+"</td>" ;
            html+="<td>"+oTable[count].Cuidador.toString()+"</td>";
            html+="<td>"+oTable[count].Distribuidora.toString()+"</td>" ;
            html+="<td>"+oTable[count].Ultima_Compra.toString()+"</td>";

            html+="<td>"+oTable[count].Telefone1.toString()+"</td>" ;
            html+="<td>"+oTable[count].Telefone2.toString()+"</td>";
            html+="<td>"+oTable[count].Data_Contato.toString()+"</td>" ;
            html+="<td>"+oTable[count].Status.toString()+"</td>";       
            html+="<td>"+oTable[count].Tipo_Paciente.toString()+"</td>";

            html+="<td>"+oTable[count].Data_Cadastro.toString()+"</td>" ;
            html+="<td>"+oTable[count].Obs.toString()+"</td>";
            html+="<td>"+oTable[count].Fase_Cadastro.toString()+"</td>";
            html+="<td>"+oTable[count].Adicionado_CRM.toString()+"</td>";
            html+="<td>"+oTable[count].Data_Insercao.toString()+"</td>";
            html+="<td>"+oTable[count].Inserido_Por.toString()+"</td>";
            html+="</tr>";
        }
        html+="</table></html>"; 

        window.open('data:application/vnd.ms-excel;base64,' + base64_encode(html));
    }
    
    function base64_encode (data) {
        var b64 = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=";
        var o1, o2, o3, h1, h2, h3, h4, bits, i = 0, ac = 0, enc = "", tmp_arr = [];

        if (!data) {
            return data;
        }

        do {
            o1 = data.charCodeAt(i++);
            o2 = data.charCodeAt(i++);
            o3 = data.charCodeAt(i++);
            bits = o1 << 16 | o2 << 8 | o3;
            h1 = bits >> 18 & 0x3f;
            h2 = bits >> 12 & 0x3f;
            h3 = bits >> 6 & 0x3f;
            h4 = bits & 0x3f;
            tmp_arr[ac++] = b64.charAt(h1) + b64.charAt(h2) + b64.charAt(h3) + b64.charAt(h4);
        } while (i < data.length);
        enc = tmp_arr.join('');
        var r = data.length % 3;
        return (r ? enc.slice(0, r - 3) : enc) + '==='.slice(r || 3);
    }
    
</script>
