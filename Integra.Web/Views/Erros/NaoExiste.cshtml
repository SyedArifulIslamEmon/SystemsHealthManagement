@model HandleErrorInfo
@{
    ViewBag.Title = ": Painel de Controle Íntegra Medical :";
    Layout = "~/Views/Shared/_LayoutMaster.error.cshtml";
}
<div class="row-fluid show-grid">
    <div class="span4"></div>
    <div class="error-container span5">
        <div class="well">
            <h1 class="grey lighter smaller">
                <span class="blue bigger-125"><i class="icon-sitemap"></i>404</span> Página não encontrada!
            </h1>
            <hr />
            <h4 class="lighter smaller">Nós procuramos em todos os lugares, mas não conseguimos encontrá-la!</h4>

            <div>
                <h4 class="smaller">Tente executar um dos seguintes passos:</h4>
                <ul class="unstyled spaced inline bigger-110">
                    <li><i class="icon-hand-right blue"></i>Verifique novamente o endereço digitado</li>
                    <li><i class="icon-hand-right blue"></i>Conte-nos sobre isto</li>
                </ul>
            </div>

            <hr />
            <div class="space"></div>

            <div class="row-fluid">
                <div class="center">
                    <a href="@Url.Action("Index", "Home", new { })" class="btn btn-grey">
                        <i class='icon-home'></i>Ínicio
                    </a>
                </div>
            </div>
        </div>
    </div>
    <div class="span4"></div>
</div>
